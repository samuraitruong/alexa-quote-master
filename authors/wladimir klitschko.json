            {
                "name": "Rules",
                "url": "/topics/rules"
            },
            {
                "name": "Other",
                "url": "/topics/other"
            }
        ]
    },
    {
        "url": "/quotes/wladimir_klitschko_602285",
        "quote": "I'm playing right now a role, and the role is called the 'Heavyweight Champion of the World.' And it takes all of the time. And I love this role, and it takes a lot of attention for me for the sport, and I just don't want to lose the title, so that's why I have to stay focused and not become an actor.",
        "author": {
            "name": "Wladimir Klitschko",
            "url": "/quotes/wladimir_klitschko_602285"
        },
        "topics": [
            {
                "name": "Love",
                "url": "/topics/love"
