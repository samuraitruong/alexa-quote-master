            {
                "name": "Which",
                "url": "/topics/which"
            },
            {
                "name": "Yogi",
                "url": "/topics/yogi"
            }
        ]
    },
    {
        "url": "/quotes/paramahansa_yogananda_392261",
        "quote": "There are always two forces warring against each other within us.",
        "author": {
            "name": "Paramahansa Yogananda",
            "url": "/quotes/paramahansa_yogananda_392261"
        },
        "topics": [
            {
                "name": "Always",
                "url": "/topics/always"
