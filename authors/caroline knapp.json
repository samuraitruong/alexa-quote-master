            {
                "name": "Some",
                "url": "/topics/some"
            },
            {
                "name": "Friday Night",
                "url": "/topics/friday_night"
            }
        ]
    },
    {
        "url": "/quotes/caroline_knapp_537915",
        "quote": "You'll reach into your wallet to brandish a photograph of a new puppy, and a friend will say, 'Oh, no - not pictures.'",
        "author": {
            "name": "Caroline Knapp",
            "url": "/quotes/caroline_knapp_537915"
        },
        "topics": [
            {
                "name": "New",
                "url": "/topics/new"
